[program:{{ supervisor_program }}]
command={{ supervisor_command }}
autostart=true
autorestart=true
startretries={{ supervisor_restart_retries }}
stderr_logfile={{ supervisor_log_dir }}/{{ supervisor_program }}.err.log
stderr_logfile_maxbytes= {{ supervisor_logfile_maxbytes }}
stderr_logfile_backups={{ supervisor_logfile_backups }}
stdout_logfile={{ supervisor_log_dir }}/{{ supervisor_program }}.out.log
stdout_logfile_maxbytes= {{ supervisor_logfile_maxbytes }}
stdout_logfile_backups={{ supervisor_logfile_backups }}
user={{ supervisor_user }}
environment={{ supervisor_environment }}
